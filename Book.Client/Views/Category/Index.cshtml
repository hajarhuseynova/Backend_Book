@model IEnumerable<CategoryGetInfoDto>

@{

    int count = 0;
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
          
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
                count++;
            <tr>
                <th scope="row">@count</th>
                <td>@item.Name</td>
               
            </tr>
        }

    </tbody>
</table>